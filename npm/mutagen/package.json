{
  "name": "@nuanced-dev/mutagen",
  "version": "0.19.0-dev.1",
  "description": "Fast file synchronization and network forwarding for remote development",
  "license": "MIT",
  "author": "Mutagen IO",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuanced-dev/mutagen.git"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "nuanced-mutagen": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsx src/cli.ts",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "dependencies": {
    "child-process-promise": "^2.2.1"
  },
  "devDependencies": {
    "@types/child-process-promise": "^2.2.6",
    "@types/node": "^20.19.30",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0"
  },
  "optionalDependencies": {
    "@nuanced-dev/mutagen-darwin-arm64": "0.19.0-dev.0",
    "@nuanced-dev/mutagen-darwin-x64": "0.19.0-dev.0",
    "@nuanced-dev/mutagen-linux-arm64": "0.19.0-dev.0",
    "@nuanced-dev/mutagen-linux-x64": "0.19.0-dev.0",
    "@nuanced-dev/mutagen-win32-arm64": "0.19.0-dev.0",
    "@nuanced-dev/mutagen-win32-x64": "0.19.0-dev.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
